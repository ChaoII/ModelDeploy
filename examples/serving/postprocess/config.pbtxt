name: "postprocess"
backend: "python"
max_batch_size: 4
dynamic_batching {
  max_queue_delay_microseconds: 50000  # 等待 10ms 批处理（单位：微秒）
}
input [
  {
    name: "POST_INPUT_0"
    data_type: TYPE_FP32
    dims: [-1, -1]
  },
  {
    name: "POST_INPUT_1"
    data_type: TYPE_STRING
    dims: [-1]
  }
]

output [
  {
    name: "POST_OUTPUT"
    data_type: TYPE_STRING
    dims: [ -1 ]
  }
]

instance_group [
  {
      count: 1
      kind: KIND_CPU
  }
]