@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# 查找依赖
if(@BUILD_VISION@)
    find_dependency(OpenCV REQUIRED)
endif()

# 包含目标文件
include("${CMAKE_CURRENT_LIST_DIR}/ModelDeploySDKTargets.cmake")

# 设置包含目录
set_and_check(ModelDeploySDK_INCLUDE_DIR "@PACKAGE_INSTALL_INCLUDE_DIR@")
set_and_check(ModelDeploySDK_LIB_DIR "@PACKAGE_INSTALL_LIB_DIR@")
set_and_check(ModelDeploySDK_BIN_DIR "@PACKAGE_INSTALL_BIN_DIR@")

# 设置版本信息
set(ModelDeploySDK_VERSION "@PACKAGE_VERSION@")

# 检查所有必需的变量
check_required_components(ModelDeploySDK) 